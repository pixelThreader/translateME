{
  "version": 2,
  "builds": [
    { "src": "Translator/wsgi.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "templates/index.html"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "Translator.settings"
  },
  "functions": {
    "api": {
      "runtime": "python3.9"
    }
  }
}
