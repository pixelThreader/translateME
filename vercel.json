{
  "version": 2,
  "builds": [
    {
      "src": "Translator",
      "use": ".vercel/build/translator",
      "config": {
        "buildCommand": "django-admin build -c production",
        "runtime": "python:3.9",
        "environment": {
          "DJANGO_SETTINGS_MODULE": "translator.settings.production"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "templates/index.html",
      "continue": true
    }
  ]
}
